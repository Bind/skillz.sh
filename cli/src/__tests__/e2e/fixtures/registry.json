{
  "name": "test-registry",
  "version": "2.0.0",
  "skills": [
    {
      "name": "tmux",
      "description": "Control tmux sessions from AI agents",
      "version": "1.0.0",
      "files": {
        "skill": ["SKILL.md", "skill.json"],
        "entry": {
          "list-sessions": "src/tmux/list-sessions.ts",
          "create-session": "src/tmux/create-session.ts",
          "send-command": "src/tmux/send-command.ts",
          "capture-output": "src/tmux/capture-output.ts",
          "kill-session": "src/tmux/kill-session.ts"
        }
      },
      "domain": "terminal",
      "utils": ["utils", "tmux"]
    },
    {
      "name": "linear-issues-read",
      "description": "List and get Linear issues via CLI (read-only operations)",
      "version": "1.0.0",
      "files": {
        "skill": ["SKILL.md", "skill.json"],
        "entry": {
          "list-issues": "src/linear/list-issues.ts",
          "get-issue": "src/linear/get-issue.ts"
        }
      },
      "domain": "linear",
      "utils": ["utils", "linear"],
      "dependencies": {
        "@linear/sdk": "^29.0.0"
      },
      "setup": {
        "env": ["LINEAR_API_KEY"],
        "instructions": "Get your API key from Linear Settings > API > Personal API keys"
      }
    },
    {
      "name": "linear-issues-write",
      "description": "Create and update Linear issues via CLI (write operations)",
      "version": "1.0.0",
      "files": {
        "skill": ["SKILL.md", "skill.json"],
        "entry": {
          "create-issue": "src/linear/create-issue.ts",
          "update-issue": "src/linear/update-issue.ts"
        }
      },
      "domain": "linear",
      "utils": ["utils", "linear"],
      "dependencies": {
        "@linear/sdk": "^29.0.0"
      },
      "setup": {
        "env": ["LINEAR_API_KEY"],
        "instructions": "Get your API key from Linear Settings > API > Personal API keys"
      }
    }
  ],
  "agents": [],
  "utils": ["utils.ts", "linear.ts", "tmux.ts"]
}
